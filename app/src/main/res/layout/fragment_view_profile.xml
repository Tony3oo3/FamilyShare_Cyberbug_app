<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cyberbug.view.ViewProfileFrag">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txt_view_birthdate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="@+id/label_profile_birthdate"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_birthdate" />

            <TextView
                android:id="@+id/txt_view_surname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="@+id/label_profile_surname"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_surname" />

            <TextView
                android:id="@+id/label_profile_name"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="53dp"
                android:layout_marginStart="56dp"
                android:layout_marginTop="85dp"
                android:gravity="center_vertical"
                android:text="@string/name"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/label_profile_surname"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="53dp"
                android:layout_marginTop="85dp"
                android:layout_marginEnd="92dp"
                android:gravity="center_vertical"
                android:text="@string/lastname"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/label_profile_gender"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="89dp"
                android:layout_height="54dp"
                android:layout_marginStart="56dp"
                android:layout_marginTop="228dp"
                android:gravity="center_vertical"
                android:text="@string/sex"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_view_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="@+id/label_profile_name"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_name" />

            <TextView
                android:id="@+id/label_profile_birthdate"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="54dp"
                android:layout_marginTop="230dp"
                android:layout_marginEnd="64dp"
                android:gravity="center_vertical"
                android:text="@string/birthdate"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/label_profile_phone"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="54dp"
                android:layout_marginTop="520dp"
                android:gravity="center_vertical"
                android:text="@string/phone_number"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_view_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_phone" />

            <TextView
                android:id="@+id/label_profile_email"
                style="@style/Widget.AppCompat.TextView.SpinnerItem"
                android:layout_width="wrap_content"
                android:layout_height="54dp"
                android:layout_marginTop="380dp"
                android:gravity="center_vertical"
                android:text="@string/email"
                android:textIsSelectable="true"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_view_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_email" />

            <TextView
                android:id="@+id/txt_view_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="12dp"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="@+id/label_profile_gender"
                app:layout_constraintTop_toBottomOf="@+id/label_profile_gender" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>



